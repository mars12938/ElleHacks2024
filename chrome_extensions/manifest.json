{
  "manifest_version": 3,
  "name": "Lumoslite",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "thehappyhack.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "notifications",
    "alarms",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
